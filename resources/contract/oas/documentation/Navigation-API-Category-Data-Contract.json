		"/nav-category/v1/categories": {
			"get": {
				"tags": [
					"Category Hierarchy Exposer"
				],
				"summary": "Get L1 & L2 category details using Store and Department(s)\n",
				"operationId": "getCategoryHierarchyDetailsUsingGET",
				"produces": [
					"application/json"
				],
				"parameters": [
					{
						"name": "departments",
						"in": "query",
						"description": "Department Name separated by comma. e.g Women,Men",
						"required": false,
						"type": "string"
					},
					{
						"name": "departmentADD",
						"in": "query",
						"description": "Department Name separated by comma. e.g Women,Men",
						"required": false,
						"type": "string"
					},
					{
						"name": "departmentName",
						"in": "query",
						"description": "Department Name separated by comma. e.g Women,Men",
						"required": false,
						"type": "string"
					},
					{
						"name": "storeId",
						"in": "query",
						"description": "Country code",
						"required": true,
						"type": "string",
						"default": "UK",
						"enum": [
							"IE",
							"UK"
						]
					}
				],
				"responses": {
					"200": {
						"description": "Success",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/CategoryHierarchy"
							}
						}
					},
					"400": {
						"description": "Bad Request"
					},
					"401": {
						"description": "Unauthorized"
					},
					"403": {
						"description": "Forbidden"
					},
					"404": {
						"description": "Resource not found"
					},
					"500": {
						"description": "Failed to get category lookup content"
					}
				}
			}
		},
